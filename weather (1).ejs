      align-items:center;
      justify-content:center;
      flex-direction:column;
      border-radius:10px; 
      box-shadow: -19px 16px 9px -4px rgba(245,166,35,1);
      transition:all all 1.5s cubic-bezier(0.7,-.5,0.3,1.8);
      transform-style: preserve-3d;
      cursor: pointer;  
      perspective:1000px;
    }
    #down1:hover{
      background-color:black;
      transform: rotateY(180deg);
    }
    #front,#back{
      position:relative;
      backface-visibility:hidden;
    }
    #front{
      cursor:pointer;
      height:100%;
      backface-visibility:hidden;
      display:flex;
      align-items:center;
      justify-content:center;
      flex-direction:column;
    }
    #back{
      transform:rotateY(180deg);
      position:absolute;
      background:black;
      height:100%;
      border-radius:10px;
    }
